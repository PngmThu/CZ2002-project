import java.io.Serializable;

public class Seat implements Serializable{

	private String type;
	private int row;
	private int number;
	
	public Seat(String type, int row, int number) {
		this.type = type;
		this.row = row;
		this.number = number;
	}

	public String getType() {
		return this.type;
	}

	/**
	 * 
	 * @param type
	 */
	public void setType(String type) {
		this.type = type;
	}

	public int getRow() {
		return this.row;
	}

	/**
	 * 
	 * @param row
	 */
	public void setRow(int row) {
		this.row = row;
	}

	public int getNumber() {
		return this.number;
	}

	/**
	 * 
	 * @param number
	 */
	public void setNumber(int number) {
		this.number = number;
	}

}