import java.io.Serializable;

public class Cinema implements Serializable{

	private Seat[][] seats = new Seat[12][16];
	private ShowTime[] showTime;
	private int id;
	
	public Cinema(Seat[][] seats, ShowTime[] showTime, int id) {
		this.seats = seats;
		this.showTime = showTime;
		this.id = id;
	}

	public Seat[][] getSeats() {
		return this.seats;
	}

	/**
	 * 
	 * @param seats
	 */
	public void setSeats(Seat[][] seats) {
		this.seats = seats;
	}

	public ShowTime[] getGetShowTime() {
		return this.showTime;
	}

	/**
	 * 
	 * @param getShowTime
	 */
	public void setGetShowTime(ShowTime[] showTime) {
		this.showTime = showTime;
	}

	public int getId() {
		return this.id;
	}

	/**
	 * 
	 * @param id
	 */
	public void setId(int id) {
		this.id = id;
	}

}